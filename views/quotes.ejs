<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Quotes List</title>
  <style>
    body {
      font-family: Arial, sans-serif;
    }
    .quote-container {
      margin-bottom: 20px;
    }
    .quote {
      font-size: 1.2em;
      font-weight: bold;
    }
    .author {
      font-size: 1em;
      font-style: italic;
    }
  </style>
</head>
<body>
  <h1>Quotes</h1>
  <% quotes.forEach(quote => { %>
    <div class="quote-container">
      <div class="quote"><%= quote.quote %></div>
      <div class="author">â€” <%= quote.author %></div>
    </div>
  <% }) %>
</body>
</html>